<?xml version="1.0" encoding="utf-8"?>
<!--
  Solution-level NuGet configuration.
  Adds package-source mappings so the xUnit test packages resolve from
  nuget.org even when the global NuGet.Config has a stale source mapping.
-->
<configuration>
  <packageSourceMapping>
    <packageSource key="Nuget Official">
      <package pattern="xunit*" />
      <package pattern="Microsoft.NET.Test.Sdk" />
      <package pattern="coverlet*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
